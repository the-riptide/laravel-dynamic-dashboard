{
  "name": "the-riptide/laravel-dynamic-dashboard",
  "description": "A way to generate a dynamic dashboard quickly and easily",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "laravel",
    "dashboard",
    "dynamic"
  ],
  "authors": [
    {
      "name": "Jelte ten Holt",
      "email": "jelte@theriptide.net",
      "homepage": "https://www.theriptide.net/",
      "role": "developer"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "TheRiptide\\LaravelDynamicDashboard\\DynamicDashboardServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "TheRiptide\\LaravelDynamicDashboard\\": "src/",
      "TheRiptide\\LaravelDynamicDashboard\\Tests\\": "tests"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.26",
    "livewire/livewire": "^2.5",
    "intervention/image": "^2.7",
    "the-riptide/laravel-dashboard-components": "^0.1.11",
    "doctrine/dbal": "^3.3"
  }

}
